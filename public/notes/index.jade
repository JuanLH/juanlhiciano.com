- posts = partial('../_shared/posts')
ul.List.Post-List
  - post = posts.shift();
  while post
    li.List-item
      .List-meta.Post-meta: time.Timestamp #{moment(post.date).format('MMMM DD, YYYY')}
      .List-text.Post-title: a(href="#{post.url_relative}") #{post.title}
    - post = posts.shift();
