- var tags = public.tags
- tags = tags._data ? Object.keys(tags._data) : [];
each tag in tags
  -   tagPosts = partial('../_shared/posts', { filter: function (post) {
  -     if (post.tags && post.tags.indexOf(tag) !== -1) return true;
  -     return false;
  -   }});

  h3= tag
  ul.List
    - post = tagPosts.shift();
    while post
      li.List-item
        .List-meta: time.Timestamp #{moment(post.date).format('MMMM DD, YYYY')}
        .List-text: a(href="#{post.url_relative}") #{post.title}
      - post = tagPosts.shift();